<h3>EventCard</h3>

<div class="card">
    <h3>@EventName</h3>
    <p>Date: @EventDate</p>
    <p>Location: @EventLocation</p>
</div>

@code {
    [Parameter] 
    public string EventName { get; set; }
    
    [Parameter] 
    public string EventDate { get; set; }
    
    [Parameter]
    public string EventLocation { get; set; }
}
